<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pg.autyzm.graprzyjazneemocje"
    android:sharedUserId="autyzm.pg">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!--uses-permission android:name="android.permission.INTERACT_ACROSS_USERS_FULL"/-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/p"
        android:label="@string/app_name"
        android:screenOrientation="landscape"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

            </intent-filter>
        </activity>
        <activity
            android:name=".RewardAndHintActivity"
            android:screenOrientation="landscape" />
        <activity
        android:name=".animation.AnimationActivity"
        android:screenOrientation="landscape" />
        <activity
            android:name=".animation.AnimationEndActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".EndActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".LevelFailedActivity"
            android:label="@string/title_activity_level_failed"
            android:screenOrientation="landscape" />
        <activity
        android:name=".VideoWelcomeActivity"
        android:screenOrientation="landscape" />
        <activity
            android:name=".Blank"
            android:screenOrientation="landscape" />
    </application>

</manifest>